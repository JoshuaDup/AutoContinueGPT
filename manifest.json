{
    "manifest_version": 3,
    "name": "Auto Continue GPT",
    "version": "1.0",
    "permissions": ["scripting", "tabs", "activeTab"],
    "host_permissions": ["https://chat.openai.com/c/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "ExtensionLogo.png",
        "48": "ExtensionLogo.png",
        "128": "ExtensionLogo.png"
      }
    }
  }
  